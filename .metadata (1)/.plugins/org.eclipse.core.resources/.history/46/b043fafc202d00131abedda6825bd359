// CS 2510 Lab 5
import tester.*;

// to represent a calendar date
public class Date {
  int year;
  int month;
  int day;

  Date(int year, int month, int day){
    this.year = year;
    this.month = month;
    this.day = day;
  }
  
  //validNumber
  public boolean validNumber(int num, int low, int high) {
      return (num >= low) && (num <= high);
  }
  
  //validDay
  public boolean validDay(int day, int low, int high) {
      return (day >= low) && (day <= high);
  }
  
  //validMonth
  public boolean validMonth(int month, int low, int high) {
      return (month >= low) && (month <= high);
  }
  
  //validYear
  public boolean validYear(int year, int low, int high) {
      return (year >= low) && (year <= high);
  }
}